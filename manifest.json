{
    "name": "Lewis' work timer",
    "manifest_version": 3,
    "version": "1.0.1",
    "description": "Extension to start a work timer to increase productivity",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "background"
    ]
}